#VRML V2.0 utf8

# http://www.bellrock.org.uk/lighthouse/lighthouse_lightroom.htm

Switch {
	choice [
		DEF	Chandelier Inline {
			url	["chandelier.wrl"]
		}
		DEF	Cage Inline	{
			url	["lightroom-cage.wrl"]
		}
		DEF	Masonry Inline {
			url	["lightroom-masonry.wrl"]
		}
	]
}

USE	Masonry

Transform {
	children [
		USE	Cage
		Transform {
			children [
				USE	Chandelier
			]
			translation	0 2.037 0
		}
	]
	translation	0 1.4478 0
}
