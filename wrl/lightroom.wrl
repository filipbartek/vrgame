#VRML V2.0 utf8

# http://www.bellrock.org.uk/lighthouse/lighthouse_lightroom.htm

Switch {
	choice [
		DEF	Chandelier Inline {
			url	["chandelier.wrl"]
		}
		DEF	Cage Inline	{
			url	["lightroom-cage.wrl"]
		}
	]
}

Transform {
	children [
		USE	Chandelier
	]
	scale 0.8 0.8 0.8
}

Transform {
	children [
		USE	Cage
	]
	scale 1.5 1.5 1.5
}
